                    </div>
                    <div>
                      <h1 class="text-2xl font-bold">
                        <%= project.name %>
                      </h1>
                      <div class="flex items-center mt-1">
                        <span id="deployment-status"
                          class="px-2 py-1 text-xs rounded-full bg-yellow-900 text-yellow-300 mr-2">Building</span>
                        <span class="text-gray-400 text-sm">
                          <script>document.write(formatTime("<%= project.createdAt %>"));</script>
                        </span>
                      </div>
                    </div>
                  </div>
                  <div class="mt-4 md:mt-0">
                    <div class="flex items-center text-base">
                      <svg class="w-4 h-4 text-gray-400 mr-1.5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
                        fill="currentColor">
                        <path
                          d="M10 6V8H5V19H16V14H18V20C18 20.5523 17.5523 21 17 21H4C3.44772 21 3 20.5523 3 20V7C3 6.44772 3.44772 6 4 6H10ZM21 3V12L17.206 8.207L11.2071 14.2071L9.79289 12.7929L15.792 6.793L12 3H21Z">
                        </path>
                      </svg>
                      <a target="_blank" class="hover:text-blue-400 underline"
                        href="https://<%= project.instanceData.subdomain %>">
                        <%= project.instanceData.subdomain %>
                      </a>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Project Details -->
              <div class="grid grid-cols-1 md:grid-cols-2 gap-6 mb-8">
                <!-- Source Details -->
                <div class="bg-dark-800 rounded-lg border border-dark-600 overflow-hidden">
                  <div class="p-4 border-b border-dark-600">
                    <h2 class="text-lg font-medium">Source</h2>
                  </div>
                  <div class="p-4">
                    <div class="space-y-3">
                      <div class="flex justify-between">
                        <span class="text-gray-400">Repository</span>
                        <a target="_blank" href="https://github.com/<%= project.repoMetaData.repoUrl %>"
                          class="text-blue-400 hover:underline truncate ml-4">
                          <%= project.repoMetaData.repoUrl %>
                        </a>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Branch</span>
                        <span>
                          <%= project.repoMetaData.repoBranch %>
                        </span>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Commit</span>
                        <a target="_blank"
                          href="https://github.com/<%= project.repoMetaData.repoUrl %>/commits/<%= project.deployments[0].commitMetaData.repoCommitHash %>"
                          class="text-blue-400 hover:underline truncate ml-4">
                          <%= project.deployments[0].commitMetaData.repoCommitHash %>
                        </a>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Commit Message</span>
                        <span class="truncate ml-4">
                          <%= project.deployments[0].commitMetaData.repoLastCommitMsg %>
                        </span>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- Build Configuration -->
                <div class="bg-dark-800 rounded-lg border border-dark-600 overflow-hidden">
                  <div class="p-4 border-b border-dark-600">
                    <h2 class="text-lg font-medium">Build Configuration</h2>
                  </div>
                  <div class="p-4">
                    <div class="space-y-3">
                      <div class="flex justify-between">
                        <span class="text-gray-400">Framework</span>
                        <span>
                          <%= project.framework %>
                        </span>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Node.js Version</span>
                        <span>20.x</span>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Build Command</span>
                        <span>
                          <%= project.deployments[0].configMetaData.buildCommand %>
                        </span>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Start Command</span>
                        <span>
                          <%= project.deployments[0].configMetaData.startCommand %>
                        </span>
                      </div>
                      <div class="flex justify-between">
                        <span class="text-gray-400">Install Command</span>
                        <span>
                          <%= project.deployments[0].configMetaData.installCommand %>
                        </span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Deployment Logs -->
              <div class="bg-dark-800 rounded-lg border border-dark-600 overflow-hidden">
                <div class="p-4 border-b border-dark-600 flex justify-between items-center">
                  <h2 class="text-lg font-medium">Deployment Logs</h2>
                  <button class="text-gray-400 hover:text-white">
                    <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24"
                      stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                        d="M8 16H6a2 2 0 01-2-2V6a2 2 0 012-2h8a2 2 0 012 2v2m-6 12h8a2 2 0 002-2v-8a2 2 0 00-2-2h-8a2 2 0 00-2 2v8a2 2 0 002 2z" />
                    </svg>
                  </button>
                </div>
                <div class="p-0">
                  <div id="logContainer" class="font-mono text-sm text-amber-500 bg-dark-900 h-96 overflow-y-auto p-4">
                    <p class="mb-1 font-mono text-gray-100 text-sm">Waiting for logs...</p>
                    <!-- Logs will be dynamically added here -->
                  </div>
                </div>
              </div>

              <!-- Action Buttons -->
              <div class="flex justify-between mt-8">
                <button
                  class="px-4 py-2 bg-white text-black font-medium rounded-md hover:bg-gray-200 transition-colors">
                  View Project
                </button>
              </div>
            </div>
          </main>
      </div>
  </div>

  <%- include('partials/footer.ejs') %>