<%- include('partials/header.ejs') %>
  <script src="https://cdn.jsdelivr.net/npm/canvas-confetti@1.6.0"></script>
  <script>
    dayjs.extend(window.dayjs_plugin_relativeTime); // Enable "fromNow()" functionality

    function formatTime(timestamp) {
      return dayjs(timestamp).fromNow(); // Works directly with Prisma timestamps
    }
  </script>
  <script>
    const socket = new WebSocket('ws://localhost:9001');
    socket.onerror = (error) => console.error("WebSocket Error:", error);

    socket.onopen = () => {
      console.log("‚úÖ Connected to WebSocket server");
      socket.send(JSON.stringify({ projectName: "<%= project.name %>" }));
    };

    socket.onmessage = (event) => {
      const data = JSON.parse(event.data);
      const logContainer = document.getElementById("logContainer");

      // Create a new log entry
      const logEntry = document.createElement("p");
      logEntry.classList.add("mb-1", "font-mono", "text-sm");

      if (data.log.includes("Deployment completed successfully!")) {
        logEntry.classList.add("text-green-500", "font-bold");
        // üéâ Trigger Confetti Animation üéâ
        confetti({
          particleCount: 300,
          spread: 120,
          origin: { y: 0.6 }, // Start a bit below the top
        });

        // Update the span element after confetti
        const statusSpan = document.getElementById("deployment-status");
        if (statusSpan) {
          statusSpan.classList.replace("bg-yellow-900", "bg-green-900");
          statusSpan.classList.replace("text-yellow-300", "text-green-300");
          statusSpan.textContent = "Live";
        }
      } else if (data.log.includes("Deployment failed")) {
        logEntry.classList.add("text-red-500", "font-bold");

        // Update the span element for failed deployment
        const statusSpan = document.getElementById("deployment-status");
        if (statusSpan) {
          statusSpan.classList.replace("bg-yellow-900", "bg-red-900");
          statusSpan.classList.replace("text-yellow-300", "text-red-300");
          statusSpan.textContent = "Failed";
        }
      } else {
        logEntry.classList.add("text-zinc-200");
      }

      logEntry.textContent = `[${new Date().toLocaleTimeString()}] ${data.log}`;

      // Append new log to the container
      logContainer.appendChild(logEntry);

      // Auto-scroll to the latest log
      logContainer.scrollTop = logContainer.scrollHeight;
    };

    socket.onclose = () => console.log("‚ùå WebSocket disconnected");

  </script>
