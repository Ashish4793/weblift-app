  <script src="https://cdn.tailwindcss.com"></script>
  <script src="https://unpkg.com/typewriter-effect@latest/dist/core.js"></script>
  <style>
    body {
        font-family: "Varela Round", sans-serif;
    }

    #typewriter {
      display: inline-block;
      min-width: 9ch; /* Adjust this based on the longest word */
      text-align: left; /* Ensures smooth transitions */
    }
  </style>

<script>
    window.onload = (event) => {
      var app = document.getElementById('typewriter');
  
      var typewriter = new Typewriter(app, {
        loop: true,
        delay: 75,
      });
  
      typewriter
        .pauseFor(100)
        .typeString('Instantly!')
        .pauseFor(1000)
        .deleteChars(10)
        .typeString('Securely!')
        .pauseFor(1000)
        .deleteChars(10)
        .typeString('Efficiently!')
        .pauseFor(1000)
        .deleteChars(10)
        .start();
    };  
</script>
